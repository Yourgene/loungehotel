

#URL Rewriting du site web
<IfModule mod_rewrite.c>

    RewriteEngine On
    RewriteRule    ^accueil/?$    index.php    [L]

    RewriteRule    ^reservation/?$    index.php?section=nouvelle_reservation    [L]

    RewriteRule    ^hebergement/?$    index.php?section=nouveau_hebergement    [L]
    

</IfModule>